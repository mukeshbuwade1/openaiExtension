{
    "name": "Answer Extension",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Provides answers along with search results",
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [{
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"]
    }]
  }